// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+shift+i",
    "command": "emojisense.quickEmojitext",
    "when": "editorReadonly\t&& !editorReadonly"
  },
  {
    "key": "ctrl+shift+i",
    "command": "-emojisense.quickEmojitext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+i",
    "command": "emojisense.quickEmoji",
    "when": "editorReadonly && !editorReadonly"
  },
  {
    "key": "ctrl+i",
    "command": "-emojisense.quickEmoji",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+i",
    "command": "emojisense.quickEmojitextTerminal",
    "when": "editorReadonly && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+i",
    "command": "-emojisense.quickEmojitextTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+i",
    "command": "emojisense.quickEmojiTerminal",
    "when": "editorReadonly && !editorReadonly"
  },
  {
    "key": "ctrl+alt+i",
    "command": "-emojisense.quickEmojiTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "shift+alt+j",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+down",
    "command": "-editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+k",
    "command": "editor.action.copyLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+up",
    "command": "-editor.action.copyLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+f12",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && !editorTextFocus && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "shift+f12",
    "command": "editor.action.goToTypeDefinition",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+shift+d",
    "command": "workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled && !viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "ctrl+shift+d",
    "command": "-workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "ctrl+shift+d",
    "command": "editor.action.addSelectionToPreviousFindMatch"
  },
  {
    "key": "ctrl+alt+s",
    "command": "cph.submitToCodeForces",
    "when": "isInDiffEditor && !isInDiffEditor"
  },
  {
    "key": "ctrl+alt+s",
    "command": "-cph.submitToCodeForces"
  },
  {
    "key": "ctrl+k ctrl+shift+f",
    "command": "editor.action.focusNextCursor"
  },
  {
    "key": "ctrl+k ctrl+shift+d",
    "command": "editor.action.focusPreviousCursor"
  },
  {
    "key": "ctrl+enter",
    "command": "explorer.newFile",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "explorer.newFolder",
    "when": "!editorFocus"
  }
]