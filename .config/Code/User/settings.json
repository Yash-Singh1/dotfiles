  "cSpell.ignoreRegExpList": ["string"],
  "C_Cpp.default.compilerPath": "/usr/bin/g++",
  "C_Cpp.default.cppStandard": "c++17",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.unicodeHighlight.invisibleCharacters": false,
  "css.lint.duplicateProperties": "warning",
  "leetcode.workspaceFolder": "/home/saiansh/.leetcode",
  "leetcode.defaultLanguage": "cpp",
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": true,
    "cpp": false,
    "rust": false,
    "dotenv": false
  },
  "shellcheck.ignorePatterns": {
    "**/*.xonshrc": true,
    "**/*.xsh": true,
    "**/*.zsh": true,
    "**/*.zshrc": true,
    "**/zshrc": true,
    "**/*.zprofile": true,
    "**/zprofile": true,
    "**/*.zlogin": true,
    "**/zlogin": true,
    "**/*.zlogout": true,
    "**/zlogout": true,
    "**/*.zshenv": true,
    "**/zshenv": true,
    "**/*.zsh-theme": true,
    "**/*.csh": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeQL.telemetry.enableTelemetry": true,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Atom One Dark",
  "where-am-i.colorful": false,
  "where-am-i.textTransfrom": "none",
  "scss.lint.unknownAtRules": "ignore",
  "svelte.enable-ts-plugin": true,
  "jest.autoRun": "off",
  "explorer.compactFolders": false,
  "workbench.startupEditor": "none",
  "cph.general.defaultLanguage": "cpp",
  "cph.language.cpp.Args": "-Wall -std=c++17 -fdiagnostics-color=always",
  "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontWeight": "500",
  "cph.general.defaultLanguageTemplateFileLocation": "/home/saiansh/competitive-programming/snippets/starter.cpp",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.schemas": [
    {
      "fileMatch": ["appsscript.json"],
      "url": "http://json.schemastore.org/appsscript"
    },
    {
      "fileMatch": [".clasp.json"],
      "url": "http://json.schemastore.org/clasp"
    }
  ],
  "files.associations": {
    "*.gs": "javascript",
    "*.ns": "javascript",
    ".vuerc": "json"
  },
  "npm.keybindingsChangedWarningShown": true,
  "vite.autoStart": false,
  "cSpell.enableFiletypes": [
    "browserslist",
    "coffeescript",
    "shellscript",
    "svelte"
  ],
  "cSpell.language": "en,en-US",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.productIconTheme": "fluent-icons",
  "editor.stickyScroll.enabled": true,
  "svelte.plugin.svelte.note-new-transformation": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "vscode-pets.position": "explorer",
  "vscode-pets.theme": "forest",
  "smartsemicolon.autoLineChange": false,
  "smartsemicolon.enable": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.inlineSuggest.showToolbar": "always"
